{
  "eslint.workingDirectories": ["./functions", "./frontend"],
  "npm.packageManager": "yarn",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "auto-close-tag.disableOnLanguage": ["typescript"],
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    ".vscode-insiders",
    "**/static/**",
    ".env"
  ],
  "files.exclude": {
    "**/*.log": true,
    "functions/lib": true,
    "**/.yarn": true,
    "**/.nuxt": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/.firebase": true
  },
  "cSpell.words": [
    "ALGOLIA",
    "Boie",
    "CYQGMBW",
    "EGUOKAMMHV",
    "IGXJR",
    "Ispc",
    "Onjnu",
    "Parens",
    "SGEK",
    "algoliasearch",
    "appspot",
    "commitlint",
    "devtool",
    "entrypoint",
    "etag",
    "firebaseapp",
    "firestore",
    "gsutil",
    "iarc",
    "instantsearch",
    "itunes",
    "keyval",
    "kvhr",
    "linkedin",
    "minecraft",
    "nuxt",
    "nuxtjs",
    "signup",
    "stylelint",
    "vuetify",
    "vuex"
  ]
}
